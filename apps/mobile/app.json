{
  "expo": {
    "name": "Uprent Plus",
    "slug": "uprent-plus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uprentplus.mobile",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to set your profile picture.",
        "NSCameraUsageDescription": "We need access to your camera to take a profile picture.",
        "NSLocationWhenInUseUsageDescription": "We use your location to show nearby properties on the map."
      },
      "associatedDomains": [
        "applinks:uprent-plug-web.vercel.app",
        "applinks:uprent-plug-web.vercel.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.uprentplus.mobile",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "uprentplus"
            },
            {
              "scheme": "https",
              "host": "uprent-plug-web.vercel.app"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "uprentplus"
            },
            {
              "scheme": "https",
              "host": "uprent-plug-web.vercel.app"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Uprent Plus to access your photos to set your profile picture."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ],
      "expo-secure-store"
    ],
    "scheme": "uprentplus",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c5b198dc-13f7-497a-8e42-97f9ca0d7a1e"
      }
    },
    "privacy": "public",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c5b198dc-13f7-497a-8e42-97f9ca0d7a1e"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
